library(lavaan)
library(semPlot)
library(dplyr)
my_data2 <- read.csv("Z:/Hannah/Ella Shuttleworth (undergrad student project)/Data/LBC1936_Healthy_Ageing_outliers_removed_residualised.csv")
#cognitive model with domain covariances
cog <- '
cognitive =~ digback_w1 + matreas_w1 + blkdes_w1 + digsym_w1 + symsear_w1
+ nart_w1 + lmtotal_w1 + vpatotal_w1 + ittotal_w1 + crtmean_w1 + spantot_w1 +
vftot_w1 + wtar_w1
# within-domain covariances
blkdes_w1 ~~ matreas_w1 # Visuospatial domain
blkdes_w1 ~~ spantot_w1
matreas_w1 ~~ spantot_w1
nart_w1 ~~ vftot_w1 #Crystalized domain
wtar_w1 ~~ vftot_w1
wtar_w1 ~~ nart_w1
lmtotal_w1 ~~ vpatotal_w1 # Verbal memory domain
lmtotal_w1 ~~ digback_w1
vpatotal_w1 ~~ digback_w1
ittotal_w1 ~~ digsym_w1 #Processing speed domain
ittotal_w1 ~~ symsear_w1
ittotal_w1 ~~ crtmean_w1
digsym_w1 ~~ symsear_w1
digsym_w1 ~~ crtmean_w1
symsear_w1 ~~ crtmean_w1
'
#Fit each model, generate SEM plot and print fit measures
cog_mod <- sem(model = cog, data = my_data2, missing = "ml.x")
#summary of model
summary(cog_mod, standardized = T)
